{
  "name": "slp-to-video",
  "version": "0.0.1",
  "description": "CLI tool for converting Slippi SLP files to video (without the use of OBS)",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "build": "shx rm -rf dist && tsc -b",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "bin": {
    "slp-to-video": "./bin/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelTornero/slp-to-video.git"
  },
  "keywords": [
    "slp",
    "slippi",
    "video",
    "super smash bros melee",
    "melee"
  ],
  "author": "MiguelTornero",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MiguelTornero/slp-to-video/issues"
  },
  "homepage": "https://github.com/MiguelTornero/slp-to-video#readme",
  "files": [
    "/bin",
    "/dist",
    "/assets"
  ],
  "optionalDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "@types/ini": "^4.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.29",
    "@types/which": "^3.0.3",
    "@types/yargs": "^17.0.32",
    "@yao-pkg/pkg": "^5.11.5",
    "fancy-test": "^3.0.14",
    "mocha": "^10.3.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@slippi/slippi-js": "^6.7.0",
    "ini": "^4.1.2",
    "which": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": "assets/**/*"
  }
}
